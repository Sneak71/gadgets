{
  "manifest_version": 2,

  "name": "Search in SZLib",
  "description": "This extension search the book shown in the current page in szlib.org.cn",
  "version": "0.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "search.html"
  },
  "permissions": [
    "activeTab",
    "http://www.szlib.org.cn/*"
  ],
  "homepage_url": "http://www.szlib.org.cn"
}